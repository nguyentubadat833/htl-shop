{
  "name": "htl-shop",
  "type": "module",
  "private": true,
  "scripts": {
    "clean": "rimraf .nuxt node_modules",
    "build": "nuxt build",
    "dev": "nuxt dev",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "prisma:reset": "npx prisma migrate reset --schema=./prisma/schema.prisma",
    "prisma:pull": "npx prisma db pull --schema=./prisma/schema.prisma",
    "prisma:migrate": "npx prisma migrate dev --name update --schema=./prisma/schema.prisma",
    "prisma:generate": "npx prisma generate --schema=./prisma/schema.prisma",
    "prisma:reset-all": "pnpm prisma:reset && pnpm prisma:migrate && pnpm prisma:generate",
    "prisma:export": "pnpm prisma migrate diff --from-empty  --to-schema-datamodel ./prisma/schema.prisma --script > ./prisma/init2-0.sql"
  },
  "dependencies": {
    "@nuxt/image": "1.11.0",
    "@nuxt/ui": "4.0.1",
    "@prisma/nuxt": "0.3.0",
    "google-auth-library": "^10.4.1",
    "nuxt": "^4.1.3",
    "rimraf": "^6.0.1",
    "typescript": "^5.9.3",
    "vue": "^3.5.22",
    "vue-router": "^4.5.1",
    "zod": "^4.1.12"
  },
  "packageManager": "pnpm@10.13.1+sha512.37ebf1a5c7a30d5fabe0c5df44ee8da4c965ca0c5af3dbab28c3a1681b70a256218d05c81c9c0dcf767ef6b8551eb5b960042b9ed4300c59242336377e01cfad"
}
